{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    
    {
      "src": "/(.*)\\.html",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)\\.(css|js|png|jpg|svg)",
      "dest": "/$1.$2"
    },

    
    {
      "src": "/api",
      "dest": "/api/index.py"
    }
  ]
}